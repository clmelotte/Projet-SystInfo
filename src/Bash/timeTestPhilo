#!/bin/bash
> timesPhilo.csv
for i in {1..5}
do
	echo -n "$i," >> timesPhilo.csv
done
echo "" >> timesPhilo.csv
for n in {1..16}
do
	for i in {1..5}
	do
		/usr/bin/time -a -o "timesPhilo.csv" -f '%e,' ./philo $n
		truncate -s-1 timesPhilo.csv
	done
	truncate -s-1 timesPhilo.csv
	echo "" >> timesPhilo.csv
done
